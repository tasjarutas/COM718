<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta name="description" content="" />
    <title>World Wines</title>

    <link
      rel="canonical"
      href="https://getbootstrap.com/docs/4.5/examples/album/"
    />

    <!-- Bootstrap core CSS -->
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
      integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z"
      crossorigin="anonymous"
    />

    <link href="css/album.css" rel="stylesheet" />
  </head>
  <body>
    <%- include('common/header'); -%>
<div class="container">
    <div class="row col-md-12 col-md-offset-2 custyle mx-auto">
        <h1 class="mx-auto"> üç∑ Wine Tasting üç∑</h1>
    <table class="table table-striped custab">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price ($)</th>
            <th>Province</th>
            <th>Points</th>
            <th>Taster Name</th>
        </tr>
    </thead>
            
                <% tastings.slice(0,50).forEach(function(tasting){ %>
                  <tr>    
                  <td><%=  tasting._id  %></td> 
                  <td><%=  tasting.title  %></td>
                  <td><%=  tasting.price  %></td>
                  <td><%=  tasting.province  %></td>
                  <td><%=  tasting.points  %></td>
                  <td><%=  tasting.taster_name  %></td>
                </tr>
                <% }); %>  
            
          
    </table>
    </div>
    
    

    <%- include('common/footer') -%>
  </body>
</html>
